    <owl:ObjectProperty rdf:about="http://schemas.ogf.org/mrs/2013/12/topology#providesVPC">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/mrs/2013/12/topology#VirtualCloudService"/>
        <rdfs:range rdf:resource="http://schemas.ogf.org/nml/2013/03/base#Topology"/>

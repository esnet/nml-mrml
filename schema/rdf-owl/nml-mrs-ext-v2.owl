    <owl:ObjectProperty rdf:about="http://schemas.ogf.org/mrs/2013/12/topology#routeFrom">
        <rdfs:domain rdf:resource="http://schemas.ogf.org/mrs/2013/12/topology#Route"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                     <owl:Class rdf:about="http://schemas.ogf.org/mrs/2013/12/topology#NetworkAddress"/>
                     <owl:Class rdf:about="http://schemas.ogf.org/nml/2013/03/base#NetworkObject"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
